class Solution {
public:
    int numIdenticalPairs(vector<int>& nums) {
        // int i=0;
        // int j=0;

        int count=0;
        int n=nums.size();
        for(int i=0;i<n-1; i++){
            // if(i<nums.size()-2){
                for(int j=i+1;j<n; j++){
                if(i<j && nums[i]==nums[j]){
                    count++;
                }
            }
            // }
            
        }
        return count;
    }
};
